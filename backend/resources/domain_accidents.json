{
	"version": "0.1",
	"prefixes": {
		"roadbase": "https://roadbase.be/vocabularies/",
		"schema": "https://schema.org/",
		"xsd": "http://www.w3.org/2001/XMLSchema#"
	},

	"resources": {
		"accidents": {
			"name": "accident",
			"class": "roadbase:Accident",
			"attributes": {
				"timestamp": {
					"type": "string",
					"predicate": "xsd:dateTime"
				},
				"latitude": {
					"type": "string",
					"predicate": "schema:latitude"
				},
				"longitude": {
					"type": "string",
					"predicate": "schema:longitude"
				}
			},
			"new-resource-base": "https://roadbase.be/accidents/"
		}
	}
}
