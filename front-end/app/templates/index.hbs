{{page-title 'FrontEnd'}}
<NavBar />
<Taskbar @allowAddSign={{this.allowAddSign}} @onClick={{this.updateAllowAddSign}} />



<MapComp @signs={{this.signsOnMap}} @instances={{this.model.instances}} @onMapClick={{this.setClickedPosition}} @allowAddSign={{this.allowAddSign}} />

<AddSign::AddSignContainer
