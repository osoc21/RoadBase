{
	"version": "0.1",
	"prefixes": {
		"roadbase": "https://osoc-safe-open-roads.s.redpencil.io/vocabularies/",
		"xsd": "http://www.w3.org/2001/XMLSchema#",
		"locn": "http://www.w3.org/ns/locn#"
	},

	"resources": {
		"accidents": {
			"name": "accident",
			"class": "roadbase:Accident",
			"attributes": {
				"timestamp": {
					"predicate": "roadbase:timestamp",
					"type": "xsd:dateTime"
				},
				"location": {
					"predicate": "locn:geometry",
					"type": "url"
				}
			},
			"new-resource-base": "https://osoc-safe-open-roads.s.redpencil.io/accidents/"
		}
	}
}
