{
	"version": "0.1",
	"prefixes": {
		"roadbase": "https://roadbase.be/vocabularies/",
		"schema": "https://schema.org/",
		"lblodmow": "https://data.lblod.info/vocabularies/mobiliteit/",
		"mobiliteit": "https://data.vlaanderen.be/ns/mobiliteit#"
	},

	"resources": {
		"road-sign-instances": {
			"name": "roadSignInstance",
			"class": "roadbase:RoadSignInstance",
			"attributes": {
				"location_lat": {
					"type": "string",
					"predicate": "schema:latitude"
				},
				"location_long": {
					"type": "string",
					"predicate": "schema:longitude"
				}
			},
			"relationships": {
				"road-sign-concept": {
					"predicate": "roadbase:placeholder-predicate",
					"target": "roadSignConcept",
					"cardinality": "one"
				}
			},
			"new-resource-base": "https://roadbase.be/vocabularies/roadsigninstance"
		}
	}
}