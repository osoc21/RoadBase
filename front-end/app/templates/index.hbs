{{page-title 'FrontEnd'}}
<NavBar />
<ActionBar />
<MapComp
  @lat={{this.lat}}
  @lng={{this.lng}}
  @zoom={{this.zoom}}
  @signs={{this.signsOnMap}}
  @onMapClick={{this.setClickedPosition}}
/>

{{#if this.clickedPosition}}
  <RoadSignList @signs={{this.model.signs}} @onSelect={{this.addSignToMap}} />
{{/if}}